{
    "name": "luizgdpulz/queuemaster",
    "description": "Production-oriented hybrid queue and appointment management RESTful API",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "firebase/php-jwt": "^6.10",
        "vlucas/phpdotenv": "^5.6",
        "predis/predis": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "QueueMaster\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QueueMaster\\Tests\\": "tests/phpunit/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        },
        "audit": {
            "abandoned": "report",
            "ignored": [
                "PKSA-y2cr-5h3j-g3ys"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}